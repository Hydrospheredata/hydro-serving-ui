{
  "name": "hydro-ui",
  "version": "0.0.0",
  "license": "MIT",
  "size-limit": [
    {
      "path": "hydro-serving-ui/*.js",
      "webpack": false,
      "limit": "400 KB"
    }
  ],
  "scripts": {
    "size": "size-limit",
    "ng": "ng",
    "start": "npm run sprites && ng serve",
    "build": "npm run sprites && ng build",
