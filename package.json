    "@types/long": "^4.0.0",
    "angular2-moment": "^1.6.0",
    "chart.js": "^2.7.2",
    "core-js": "^2.5.0",
    "d3": "^5.9.2",
    "d3-scale-chromatic": "^1.3.3",
