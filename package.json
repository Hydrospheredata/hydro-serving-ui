    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-prod": "npm run sprites && ng build --prod",
    "sprites": "svg-sprite --config sprite-config.json src/assets/images/_icons/*.svg",
    "bundle-report": "webpack-bundle-analyzer hydro-serving-ui/stats.json"
  },
  "private": true,
